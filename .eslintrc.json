{
  "extends": "@rocketseat/eslint-config/react",
  "rules": {
    "no-undef": "off",
    "prettier/prettier": [
      0,
      {
        "printWidth": 90,
        "tabWidth": 2,
        "singleQuote": false,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": false
      }
    ]
  }
}
